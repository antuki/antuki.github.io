row_function <- function(entier){
retour <- NULL
if(entier%in%1:3){
nrow <- 1
} else if(entier%in%4:8){
nrow <- 2
}else if(entier%in%9:12){
nrow <- 3
}else {
nrow <- 4
}
retour$nrow <- nrow
retour$ncol <- (entier %/% nrow) + 1
return(retour)
}
